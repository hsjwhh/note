# Mysql Parameters
MYSQL_PORT_3306_TCP_ADDR=根据自己的地址填写
MYSQL_PORT_3306_TCP_PORT=3306
# 数据库及用户自行创建
MYSQL_DATABASE=snipe_it 
MYSQL_USER=snipe_it_db_user
MYSQL_PASSWORD=YOUR_snipeit_USER_PASSWORD

# Snipe-IT Settings
